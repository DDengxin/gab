    height: 110px;
}
.experience .title_bg a{
    display: block;
    width: 100%;
    height: 100%;
    cursor: pointer;
}
.title_bg a:hover img{
    opacity:0.6;
    filter: "alpha(opacity=60)";/* IE 8 */
    zoom: 1;
}
.academic .title_bg img,
.experience .title_bg img{
