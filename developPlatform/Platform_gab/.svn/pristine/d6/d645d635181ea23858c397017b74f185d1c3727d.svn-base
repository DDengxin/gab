.require_container {
    width: 100%;
    overflow: hidden;
    margin: 125px 0 40px;
}

.require_content {
    width: 1200px;
    overflow: hidden;
    margin: 0 auto;
}

.require_content h2 {
    width: 100%;
    padding-bottom: 10px;
    border-bottom: 1px solid #d7d7d7;
    font-size: 18px;
    color: #333333;
    position: relative;
    padding-left: 15px;
    font-weight: bold;
}

.require_content h2:before {
    display: inline-block;
    width: 5px;
    content: '';
    height: 25px;
    vertical-align: middle;
    margin-right: 10px;
    position: absolute;
    left: 0;
    background: rgb(232, 0, 0);
    border-radius: 5px;
}

.require_content .main_content {
    width: 100%;
    overflow: hidden;
    padding-top: 30px;
}

#tablist {
    width: 100%;
    overflow: hidden;
}

#tablist li {
    width: 50%;
    padding-bottom: 10px;
    border-bottom: 2px solid #d0d0d0;
    text-align: center;
    font-size: 24px;
    color: #333333;
    float: left;
    cursor: pointer;
    font-weight: bold;
}

.tabin {
    color: #e80000 !important;
    border-color: #e80000 !important;
}

.content-box {
    width: 100%;
    overflow: hidden;
    border: 1px solid #d0d0d0;
    border-top: none;
    position: relative;
}

.content-box .content {
    width: 100%;
    clear: both;
    color: #333;
    position: relative;
    top: 0;
    left: 0;
    display: none;
    padding: 0 20px 25px;
}

.contentin {
    display: block !important;
}

.main_content form {
    width: 100%;
    padding: 22px 30px 0 50px;
}

.main_content form ul {
    width: 100%;
    overflow: hidden;
}

.main_content form ul li {
    width: 100%;
    overflow: hidden;
    padding-left: 30px;
    color: #666666;
    font-size: 16px;
    position: relative;
    margin-bottom: 20px;
}

.main_content form ul li>div {
    width: 50%;
    float: left;
}

.main_content form ul li>div>div {
    margin-left: 0;
}

.main_content form ul li>div>span+div {
    margin-left: 20px;
}

.main_content form ul li>div:first-child>div {
    margin-left: 20px;
}

.main_content form ul li>div input{
    margin-left: 0;
}

.main_content form ul li>div>span+input{
    margin-left: 20px;
}

.main_content form ul li>div:first-child input {
    margin-left: 20px;
}

form ul li span {
    display: inline-block;
    width: 110px;
    height: 100%;
    vertical-align: middle;
}

form ul li span i {
    color: #e80000;
    float: right;
}

form ul li .addRow,
form ul li .addRowProduct{
    color: #fff;
    width: 34px;
    margin-left: 20px;
    cursor: pointer;
    text-align: center;
    padding: 0 !important;
    border: none !important;
    background: rgba(232, 0, 0, 1);
    font-weight: bold;
}
form ul li .addRow:hover,
form ul li .addRowProduct:hover{
    color: #fff;
    background: rgba(232, 0, 0, .6);
}
form ul>span{
    display: block;
}
form ul>span li input{
    margin-left: 24px !important;
}
form ul li textarea {
    display: inline-block;
    border: 1px solid #c8c8c8;
    border-radius: 5px;
    margin-left: 24px;
    padding: 5px 15px 0;
    display: inline-block;
    vertical-align: top;
    resize: none;
}

form ul li input {
    width: 333px;
    height: 34px;
    line-height: 34px;
    border-radius: 5px;
    margin-left: 20px;
    padding: 0 15px;
    outline-style: none;
    border: 1px solid #c8c8c8;
}

.webuploader-pick {
    padding: 6px 10px !important;
}

.com-sel {
    display: inline-block;
    line-height: 2;
    cursor: pointer;
    margin-left: 20px;
    border: 1px solid #c8c8c8;
    border-radius: 5px;
}

.com-opt {
    width: 333px;
    height: 34px;
    line-height: 34px;
    padding: 0 22px 0 15px;
    outline-style: none;
    color: #666666;
    font-size: 16px;
    border: none;
    outline: none;
    /*鍘绘帀榛樿鐨勪笅鎷変笁瑙�*/
    appearance: none;
    -moz-appearance: none;
    -webkit-appearance: none;
    /*娣诲姞涓嬫媺涓夎鍥炬爣*/
    background: url("../images/jiantou.png") no-repeat 310px center transparent;
    background-size: 5%;
    cursor: pointer;
}

/*娓呴櫎ie鐨勯粯璁ら�夋嫨妗嗘牱寮忔竻闄わ紝闅愯棌涓嬫媺绠ご*/

select::-ms-expand {
    display: none;
}

form ul li b {
    display: inline-block;
    padding: 0 10px;
    border: 1px solid #c8c8c8;
    font-size: 15px;
    color: #666666;
    height: 34px;
    line-height: 32px;
    text-align: center;
    margin-left: 10px;
    border-radius: 5px;
}

form ul li u {
    display: block;
    margin: 10px 0 0 133px;
    font-size: 14px;
    color: #8c8989;
}

.btn {
    width: 100%;
    height: 120px;
}

.btn .btn_content {
    position: relative;
    left: 50%;
    margin-left: -121px;
}

.btn input[type="submit"] {
    width: 242px;
    height: 45px;
    text-align: center;
    background: #e80000;
    color: #fff;
    font-size: 20px;
    margin: 40px auto 120px;
    border: none;
    cursor: pointer;
}

.btn a {
    color: #e80000;
}

.btn a:hover {
    color: #e80000;
    text-decoration: underline;
}

.btn strong {
    display: inline-block;
    vertical-align: bottom;
}

.li_center {
    display: flex;
    flex-direction: row;
    align-items: center;
}

.item .webuploadItemButtons .webuploadDownload, .item .webuploadItemButtons .webuploadDelbtn, .item .webuploadItemButtons .webuploadPreview {
    width: 45px;
    text-align: center;
}

.selectHide {
    display: none;
}

.selectShow {
    display: inline-block;
}

.webuploader-container div+div{
    width: 84px !important;
    height: 34px !important;
    left: 0 !important;
    /* margin-left: -42px; */
}
.webuploader-pick {
    padding: 2px 10px;
    border: none;
    float: none !important; 
    margin-right: 0 !important;
}
.item .webuploadinfo {
    margin-bottom: 10px;
    text-align: left;
    width: 85%;
    float: left;
}
.item .webuploadstate {
    height: auto !important;
    width: 54px;
}