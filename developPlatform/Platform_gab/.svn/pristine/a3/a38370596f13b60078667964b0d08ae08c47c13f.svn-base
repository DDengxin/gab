}
.info{
	position: relative;
}
.info:after {
	display: block;
	content: '';
	border-width: 5px;
	border-style: solid;
	border-color: #e80000 transparent transparent transparent;
	position: absolute;
	bottom: 0;
	left: 50%;
	margin-left: -5px;
}

.info .choice {
	display: none;
	width: auto;
	height: auto !important;
	position: fixed;
	top: 40px;
	/* left: 31.6%; */
	/* right: 67%; */
	background: #000;
}

.info .choice li {
	width: 100%;
	height: 30px;
	line-height: 30px !important;
	text-align: center;
	font-size: 14px !important;
	display: block !important;
	margin: 0 !important;
}

.info .choice li a {
	display: block;
	width: 100%;
	height: 100%;
	/* padding: 0 10px; */
}

.info .choice li a:hover {
	display: block;
	width: 100%;
	height: 100%;
	color: #fff !important;
	background: #e80000 !important;
}

/* 头部分类 */
.header {
	width: 100%;
	height: 63px;
	background: #f4f4f4;
	border-bottom: 1px solid #e3e3e3;
	position: fixed;
	top: 40px;
	z-index: 89;
}

.header .header_content {
	width: 1200px;
	height: 100%;
	margin: 0 auto;
}

.header .header_content a {
	color: #333333;
}

.header .header_content h1 {
	float: left;
	width: 246px;
	height: 60px;
}

.header .header_content h1 a {
	display: inline-block;
	width: 246px;
	height: 60px;
	background: url(../images/sy_03.png) no-repeat center center;
}

.header .header_content .head_nav {
	float: right;
	height: 100%;
}

.header .header_content .head_nav ul {
	height: 100%;
}

.header .header_content .head_nav ul li {
	display: inline-block;
	line-height: 63px;
	margin-right: 40px;
	font-size: 18px;
	color: #333333;
}

.header .header_content .head_nav ul .search {
	margin-right: 0;
}

.header .header_content .head_nav ul li a {
	color: #333333;
}

.header .header_content .head_nav ul li:hover a {
	color: #e80000;
}

.header .header_content .head_nav ul li:hover .choice a {
	color: #333;
}

.header .header_content .head_nav ul .search {
	width: 272px;
	height: 30px;
	border: 1px solid #e80000;
	border-radius: 15px;
	font-size: 15px;
	display: inline-block;
	line-height: 28px;
	padding-left: 15px;
	position: relative;
}

.header .header_content .head_nav ul .search i {
	display: inline-block;
	width: 19px;
	height: 19px;
	background: url(../images/sy_06.png) no-repeat center center;
	vertical-align: middle;
}

.header .header_content .head_nav ul .search input {
	height: 30px;
	line-height: 30px;
	border: none;
	background: none;
	font-size: 16px;
	width: 163px;
	margin-left: 5px;
}

.header .header_content .head_nav ul .search b {
	display: inline-block;
	color: #fff;
	line-height: 30px;
	background: #e80000;
	height: 30px;
	width: 59.1px;
	border-radius: 0 15px 15px 0;
	text-align: center;
	margin-top: -1px;
	position: absolute;
	top: 0;
	right: 0;
	cursor: pointer;
}

.choice_container, .choice_firm, .choice_manager {
	position: relative;
}

.choice_firm:after, .choice_container:after, .choice_manager:after {
	display: block;
	content: '';
	border-width: 6px;
	border-style: solid;
	border-color: #e80000 transparent transparent transparent;
	position: absolute;
	bottom: 2px;
	left: 50%;
	margin-left: -6px;
}

.choice_firm .choice, .choice_container .choice , .choice_manager .choice {
	display: none;
	width: 103px;
	height: auto !important;
	position: absolute;
	top: 63px;
	left: -12px;
	background: #f4f4f4;
	padding: 10px 0;
	z-index: 99;
}

.choice_firm .choice li, .choice_container .choice li, .choice_manager .choice li {
	width: 100%;
	height: 30px;
	line-height: 30px !important;
	text-align: center;
	font-size: 15px !important;
	display: block !important;
}

.choice_firm .choice li a, .choice_container .choice li a, .choice_manager .choice li a {
	display: block;
	width: 100%;
	height: 100%;
}

.choice_firm .choice li a:hover, .choice_container .choice li a:hover, .choice_manager .choice li a:hover {
	display: block;
	width: 100%;
	height: 100%;
	color: #fff !important;
	background: #e80000 !important;
}

/* 底部导航 */
.footer {
	width: 100%;
	overflow: hidden;
}

.footer .footer_content {
	width: 100%;
	height: 100%;
	overflow: hidden;
}

.footer .footer_content .footer_menu_list {
	width: 100%;
	height: 260px;
	background: #252a2f;
	padding-top: 35px;
}

.footer .footer_content .footer_menu_list .footer_menu_list_content {
	width: 1200px;
	height: 100%;
	margin: 0 auto;
}

.footer_menu_list .footer_menu_list_content ul {
	color: #ffffff;
	width: 254px;
	height: 100%;
	overflow: hidden;
	text-align: center;
	float: left;
	margin-right: 55px;
}

.footer_menu_list .footer_menu_list_content ul li {
	width: 100%;
	font-size: 16px;
	font-weight: bold;
	display: inline-block;
	line-height: 1.5;
}

.footer_menu_list .footer_menu_list_content ul li:first-child {
	font-size: 30px;
}

.footer_menu_list .footer_menu_list_content ul li img {
	display: inline-block;
	text-align: center;
	width: 103px;
	height: 103px;
}

.footer_menu_list .footer_menu_list_content dl {
	width: 222px;
	height: 100%;
	overflow: hidden;
	padding-left: 70px;
	position: relative;
	float: left;
}

.footer_menu_list .footer_menu_list_content dl:before {
	display: block;
	content: '';
	width: 1px;
	height: 182px;
	background: #383d42;
	position: absolute;
	left: 0;
}
.footer_menu_list .footer_menu_list_content dl:first-child:before{
	display: none;
}

.footer_menu_list .footer_menu_list_content dl dt {
	font-size: 18px;
	color: #fff;
	font-weight: bold;
	line-height: 2;
}

.footer_menu_list .footer_menu_list_content dl dd {
	font-size: 14px;
	line-height: 2;
}

.footer_menu_list .footer_menu_list_content dl dd a {
	color: #a4a9ac;
}

.footer_menu_list .footer_menu_list_content dl dd a:hover {
	color: #e80000;
}

.footer .footer_content .footer_link {
	width: 100%;
	height: 50px;
	background: #2e3339;
}

.footer .footer_content .footer_link .footer_link_content {
	width: 1200px;
	height: 100%;
	overflow: hidden;
	margin: 0 auto;
	line-height: 50px;
	font-size: 16px;
	color: #e80000;
}

.footer .footer_content .footer_link .footer_link_content a {
	color: #a4a9ac;
	margin: 0 14px;
}

.footer .footer_content .footer_link .footer_link_content a:hover {
	color: #e80000;
}

.footer .footer_content .belong {
	width: 100%;
	height: 50px;
	background: #171a1c;
	text-align: center;
	line-height: 50px;
	font-size: 14px;
	color: #a4a9ac;
}


