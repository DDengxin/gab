package com.tengzhi.business.platform.specialist.product.model;

import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;
import java.math.BigDecimal;
import java.util.Date;

/**
 * @author xutao
 * @create 2020-07-06
 */
@Entity
@Table(name = "g_product")
public class Product {
	@Id
	private String productNote;
	private String productName;
	private String sourceFactory;
	private String locality;
	private String productInfo;
	private String productType;
	private String productImage;
	private BigDecimal productSales;
	private BigDecimal productPopularity;
	private String dataCorp;
	private Date dataDate;
	private String dataMan;
	private String productFlag;
	private String supplyId;
	private BigDecimal productPrice;
	public String getProductNote() {
		return productNote;
	}
	public void setProductNote(String productNote) {
		this.productNote = productNote;
	}
	public String getProductName() {
		return productName;
	}
	public void setProductName(String productName) {
		this.productName = productName;
	}
	public String getSourceFactory() {
		return sourceFactory;
	}
	public void setSourceFactory(String sourceFactory) {
		this.sourceFactory = sourceFactory;
	}
	public String getLocality() {
		return locality;
	}
	public void setLocality(String locality) {
		this.locality = locality;
	}
	public String getProductInfo() {
		return productInfo;
	}
	public void setProductInfo(String productInfo) {
		this.productInfo = productInfo;
	}
	public String getProductType() {
		return productType;
	}
	public void setProductType(String productType) {
		this.productType = productType;
	}
	public String getProductImage() {
		return productImage;
	}
	public void setProductImage(String productImage) {
		this.productImage = productImage;
	}
	public BigDecimal getProductSales() {
		return productSales;
	}
	public void setProductSales(BigDecimal productSales) {
		this.productSales = productSales;
	}
	public BigDecimal getProductPopularity() {
		return productPopularity;
	}
	public void setProductPopularity(BigDecimal productPopularity) {
		this.productPopularity = productPopularity;
	}
	public String getDataCorp() {
		return dataCorp;
	}
	public void setDataCorp(String dataCorp) {
		this.dataCorp = dataCorp;
	}
	public Date getDataDate() {
		return dataDate;
	}
	public void setDataDate(Date dataDate) {
		this.dataDate = dataDate;
	}
	public String getDataMan() {
		return dataMan;
	}
	public void setDataMan(String dataMan) {
		this.dataMan = dataMan;
	}
	public String getProductFlag() {
		return productFlag;
	}
	public void setProductFlag(String productFlag) {
		this.productFlag = productFlag;
	}
	public String getSupplyId() {
		return supplyId;
	}
	public void setSupplyId(String supplyId) {
		this.supplyId = supplyId;
	}
	public BigDecimal getProductPrice() {
		return productPrice;
	}
	public void setProductPrice(BigDecimal productPrice) {
		this.productPrice = productPrice;
	}
}
