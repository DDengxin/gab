.container {
    width: 100%;
    overflow: hidden;
}

.container .content {
    width: 100%;
    overflow: hidden;
    padding: 15px 35px 28px 35px;
}

.container .content h2 {
    font-size: 25px;
    font-weight: bold;
    width: 100%;
    text-align: center;
    margin-bottom: 15px;
    color: #000;
}

.container .content form {
    width: 100%;
    overflow: hidden;
}

.container .content form>div {
    border-top: 1px solid #e80000;
    padding-top: 10px;
    margin-top: 20px;
}

.container .content form>div h4 {
    font-size: 19px;
    font-weight: bold;
    color: #000;
    margin-bottom: 24px;
}

.container .content form>div h4 b {
    float: right;
    font-size: 16px;
    color: rgb(99, 99, 99);
}

.container .content form .base-msg {
    width: 100%;
    overflow: hidden;
}

.container .content form .base-msg ul {
    width: 100%;
    overflow: hidden;
}

.container .content form .base-msg ul li {
    width: 100%;
    overflow: hidden;
    margin-bottom: 20px;
}

.container .content form .base-msg ul li>div {
    width: 50%;
    float: left;
    font-size: 16px;
    color: rgb(99, 99, 99);
    position: relative;
}

.container .content form .base-msg ul li div label {
    display: inline-block;
    width: 116px;
    height: 34px;
    line-height: 34px;
    vertical-align: middle;
    float: left;
}

.container .content form .base-msg ul li div input {
    width: 335px;
    height: 34px;
    border: 1px solid rgb(200, 200, 200);
    line-height: 34px;
    padding-left: 50px;
    padding-right: 12px;
    border-radius: 5px;
    text-indent: 50px;
}

.offerRemarks {
    width: 100% !important;
}

form ul li textarea {
    display: inline-block;
    border: 1px solid #c8c8c8;
    border-radius: 5px;
    padding: 5px 15px 0;
    display: inline-block;
    vertical-align: top;
    resize: none;
}

.li_center {
    display: flex;
    flex-direction: row;
    align-items: center;
}

.btn {
    width: 100%;
    margin-top: 40px !important;
    border: none !important;
    position: relative;
    height: 45px;
}

.btn #btn-submit {
    width: 242px;
    height: 45px;
    border: none;
    background: #409fff;
    color: #fff;
    line-height: 45px;
    text-align: center;
    position: absolute;
    top: 0;
    left: 50%;
    margin-left: -121px;
    font-size: 20px;
    cursor: pointer;
}

.btn #btn-submit:hover {
    background: #e80000;
}

.layui-laydate .layui-laydate-main .layui-this {
    background-color: #409fff!important;
    color: #fff!important;
}

.com-sel {
    display: inline-block;
    line-height: 2;
    cursor: pointer;
    width: 335px !important;
    border: 1px solid #c8c8c8;
    border-radius: 5px;
}

.com-opt {
    display: inline-block !important;
    width: 335px;
    height: 34px;
    line-height: 34px;
    padding: 0 22px 0 15px;
    outline-style: none;
    color: #666666;
    font-size: 16px;
    border: none;
    outline: none;
    /*鍘绘帀榛樿鐨勪笅鎷変笁瑙�*/
    appearance: none;
    -moz-appearance: none;
    -webkit-appearance: none;
    /*娣诲姞涓嬫媺涓夎鍥炬爣*/
    background: url("../images/jiantou.png") no-repeat 310px center transparent;
    background-size: 5%;
    cursor: pointer;
}

/*娓呴櫎ie鐨勯粯璁ら�夋嫨妗嗘牱寮忔竻闄わ紝闅愯棌涓嬫媺绠ご*/

select::-ms-expand {
    display: none;
}

#fileDiv {
    width: 80% !important;
    display: inline-block;
}

