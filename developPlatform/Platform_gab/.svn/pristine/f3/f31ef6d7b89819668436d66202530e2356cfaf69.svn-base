.example ul li {
    width: 100%;
    overflow: hidden;
    color: #666666;
    height: 55px;
    line-height: 55px !important;
    font-size: 16px;
    position: relative;
    border-bottom: 1px dashed #e4e4e4;
}    
.example ul li i{
    display: inline-block;
}
.example ul li span {
    display: inline-block;
    width: 80px;
    height: 24px;
    background-color: rgb(65, 160, 254);
    color: #fff;
    font-size: 15px;
    text-align: center;
    line-height: 24px;
    border-radius: 5px;
    position: absolute;
    right: 0;
    top: 50%;
    margin-top: -12px;
}
#page{
    margin-top: 65px;
}