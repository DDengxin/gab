.search_content {
    width: 100%;
    overflow: hidden;
}

.search_content_list {
    width: 100%;
    overflow: hidden;
    margin: 0 auto;
}

.showContent {
    width: 100%;
    padding-bottom: 10px;
    border-bottom: 1px solid #d7d7d7;
    font-size: 20px;
    color: #333333;
    position: relative;
    padding-left: 15px;
    font-weight: bold;
}

.showContent b {
    font-size: 20px;
    color: #333333;
    margin-right: 5px;
    font-weight: bold;
}

.showContent:before {
    display: inline-block;
    width: 5px;
    content: '';    
    height: 24px;
    margin-top: 3px;
    margin-right: 10px;
    position: absolute;
    left: 0;
    background: rgb(232, 0, 0);
    border-radius: 5px;
}

.content-box {
    width: 1200px;
    margin: 0 auto;
    position: relative;
    top: 30px;
    left: 0;
    padding: 88px 0 120px;
}

.content-box .content {
    width: 100%;
    clear: both;
    position: relative;
    top: 0;
    left: 0;
    overflow: hidden;
}

.content-box .content .fl {
    float: left;
    width: 800px;
}

.content-box .content ul {
    width: 100%;
    overflow: hidden;
}

.content-box .content ul li {
    width: 100%;
    height: 203px;
    border-top: 1px solid #efefef;
    padding: 30px 0;
}

.content-box .content ul li a {
    display: block;
    width: 100%;
    height: 100%;
    overflow: hidden;
}

.content-box .content ul li:hover a .title {
    color: #ec0a0a;
    text-decoration: underline;
}

.content-box .content ul li a div {
    width: 100%;
    overflow: hidden;
    font-size: 14px;
    color: #666666;
}

.content-box .content ul li a div .title {
    font-size: 18px;
    color: #409fff;
    font-weight: bold;
    margin-bottom: 26px;
}

.content-box .content ul li a div .from {
    margin-bottom: 17px;
}

.content-box .content ul li a div .zy {
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 3;
    overflow: hidden;
}

.fr {
    width: 305px;
    overflow: hidden;
    float: right;
    margin-top: 35px;
}

.fr dl {
    width: 100%;
    border: 1px solid #ececec;
    margin-bottom: 20px;
    padding: 20px;
}

.fr dl dt {
    font-size: 18px;
    color: #409fff;
    margin-bottom: 12px;
}

.fr dl dt:before {
    content: '';
    display: inline-block;
    width: 3px;
    height: 20px;
    background: #409fff;
    vertical-align: text-top;
    margin-right: 10px;
}

.fr dl dd {
    font-size: 14px;
    line-height: 2;
}

.fr dl dd a {
    display: block;
    color: #666;
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.fr dl dd a:hover {
    color: #ec0a0a;
    text-decoration: underline;
}

#page1 {
    position: relative;
    height: 30px;
    margin-top: 30px;
}

.layui-laypage .layui-laypage-curr .layui-laypage-em {
    background-color: rgb(232, 0, 0) !important;
}