.container {
    width: 100%;
    overflow: hidden;
}

.container .content {
    width: 100%;
    overflow: hidden;
    padding: 15px 35px 28px 35px;
}

.container .content h2 {
    font-size: 25px;
    font-weight: bold;
    width: 100%;
    text-align: center;
    margin-bottom: 15px;
    color: #000;
}

.container .content form {
    width: 100%;
    overflow: hidden;
}

.container .content form>div {
    border-top: 1px solid #e80000;
    padding-top: 10px;
    margin-top: 20px;
}

.container .content form>div h4 {
    font-size: 19px;
    font-weight: bold;
    color: #000;
    margin-bottom: 24px;
}

.container .content form>div h4 b {
    float: right;
    font-size: 16px;
    color: rgb(99, 99, 99);
}

.container .content form .base-msg, .indirect-cost {
    width: 100%;
    overflow: hidden;
}

.container .content form .base-msg ul, .indirect-cost ul {
    width: 100%;
    overflow: hidden;
}

.container .content form .base-msg ul li, .indirect-cost ul li {
    width: 100%;
    overflow: hidden;
    margin-bottom: 20px;
}

.container .content form .base-msg ul li div, .indirect-cost ul li div {
    width: 50%;
    float: left;
    font-size: 16px;
    color: rgb(99, 99, 99);
}

.indirect-cost ul li div {
    width: 33.3% !important;
}

.container .content form .base-msg ul li div label, .indirect-cost ul li div label {
    display: inline-block;
    width: 116px;
    height: 34px;
    line-height: 34px;
    vertical-align: middle;
    float: left;
}

.indirect-cost ul li div:first-child label {
    width: 132px;
    text-align: left;
}

.indirect-cost ul li div label {
    width: 120px;
    text-align: right;
    padding-right: 20px;
}

.container .content form .base-msg ul li div input, .indirect-cost ul li div input {
    width: 335px;
    height: 34px;
    border: 1px solid rgb(200, 200, 200);
    line-height: 34px;
    padding-left: 16px;
    border-radius: 5px;
}

.indirect-cost ul li div input {
    width: 200px;
}

table {
    width: 100%;
    overflow: hidden;
}

table tr {
    width: 100%;
    overflow: hidden;
    padding-bottom: 20px;
    height: 54px;
}

table tr:first-child td {
    text-align: center;
    height: 24px;
    line-height: 24px;
}

table tr td {
    height: 34px;
    line-height: 34px;
    font-size: 15px;
    color: #636366;
    padding-left: 10px;
}

table tr td:first-child {
    padding-left: 0;
}

table tr td input {
    width: 100%;
    height: 34px;
    line-height: 34px;
    border: 1px solid rgb(200, 200, 200);
    border-radius: 5px;
    text-indent: 15px;
}

.kind {
    width: 120px;
}

.cost {
    width: 110px;
}

.number, .price, .unit, .money {
    width: 85px;
}

.remarks {
    width: 200px;
}

.indirect-cost {
    margin-top: 30px !important;
}

.indirect-cost table tr td {
    width: 12.5% !important;
    position: relative;
}

.indirect-cost table tr td input {
    padding-right: 20px;
}

.indirect-cost table tr td b {
    position: absolute;
    right: 6px;
    top: 0;
    display: block;
    width: 10px;
    height: 100%;
    line-height: 54px;
}

.costTotal {
    width: 100px !important;
    margin-right: 5px;
    padding: 0 !important;
}

.leadTime {
    color: #409fff;
}

.leadTime input {
    width: 60px !important;
    padding: 0 !important;
}

.pri {
    color: #fff !important;
}

.pri input {
    width: 180px !important;
    background: #409fff;
    text-align: center;
    border: none;
    color: #fff;
    font-weight: bold;
}

.indirect-cost table tr .pri b {
    position: absolute;
    left: 17px;
    top: 0;
    font-weight: bold;
}

input[name="chineseWord"] {
    width: 500px !important;
}

.vatTotal {
    width: 40px !important;
    padding-right: 0 !important;
    /* text-align: center !important; */
}

.prescription {
    width: 60px !important;
    text-align: center;
    padding: 0 !important;
    margin: 0 5px;
}

.btn {
    width: 100%;
    margin-top: 40px !important;
    border: none !important;
    position: relative;
    height: 45px;
}

.btn #btn-submit {
    width: 242px;
    height: 45px;
    border: none;
    background: #409fff;
    color: #fff;
    line-height: 45px;
    text-align: center;
    position: absolute;
    top: 0;
    left: 50%;
    margin-left: -121px;
    font-size: 20px;
    cursor: pointer;
}
.btn #btn-submit:hover {
    background: #e80000;
}

#layui-laydate1 {
    top: 1250px !important;
}

#layui-laydate2 {
    top: 1305px !important;
}

.layui-laydate-content tr {
    height: 30px !important;
    line-height: 30px !important;
}

.layui-laydate-content tr td {
    padding: 0 !important;
}

.layui-laydate .layui-laydate-main .layui-this {
    background-color: #409fff!important;
    color: #fff!important;
}

input[name="chineseWord"]::-webkit-input-placeholder {
    /* WebKit browsers */
    color: #fff;
}

input[name="chineseWord"]:-moz-placeholder {
    /* Mozilla Firefox 4 to 18 */
    color: #fff;
}

input[name="chineseWord"]::-moz-placeholder {
    /* Mozilla Firefox 19+ */
    color: #fff;
}

input[name="chineseWord"]:-ms-input-placeholder {
    /* Internet Explorer 10+ */
    color: #fff;
}

input[name="chineseWord"]:focus::-webkit-input-placeholder {
    color: #fff;
}

