}

.com-sel {
    display: inline-block;
    line-height: 2;
    cursor: pointer;    
    width: 334px !important;
    border: 1px solid #c8c8c8;
    border-radius: 5px;
}

.com-opt {
    display: inline-block !important;
    width: 334px;
    height: 34px;
    line-height: 34px;
    padding: 0 22px 0 15px;
    outline-style: none;
    color: #666666;
    font-size: 16px;
    border: none;
    outline: none;
    /*鍘绘帀榛樿鐨勪笅鎷変笁瑙�*/
    appearance: none;
    -moz-appearance: none;
    -webkit-appearance: none;
    /*娣诲姞涓嬫媺涓夎鍥炬爣*/
    background: url("../images/jiantou.png") no-repeat 310px center transparent;
    background-size: 5%;
    cursor: pointer;
}

