<script type="text/javascript">
	mini.parse();
	function ApprovalAgreeHandler() {
		_ApprovalActionHandler_('/platform/enroll/agree');///system/workflow/process/agree
	}		
	//用户基础信息
	var user_form = new mini.Form('userform');
	setFormReadOnly(user_form,null,true)	
	//默认隐藏所有厂商表单
	$("#mixsupply,#fixturesupply,#auxiliarysupply,#strengsupply").hide();	
	//动态填充form表单数据
	function  renderForm(id,data){
		$("#"+id).show();
	   var form =	new mini.Form('#'+id);
	    form.setData(data);
		setFormReadOnly(form,null,true)		
		$("#"+id+" .headImg").filesUpload({
			pickerid :data.headImg,
             filecount : 1,
             view : "view",         
         });
		$("#"+id+" .supplyImg").filesUpload({
			pickerid :data.supplyImg,
             filecount : 1,
             view : "view",         
         });
		
	}
	
	function SetData(options) {						
		if ('sh' == options.action) {
			$.get("/platform/enroll/approve/{0}".format(options.data.businessId), function(res) {
				if (200 === res.code) {					
					user_form.setData(res.data.user);
					var data = res.data.data;
					if(data.supplyType == "夹具厂商" ){
						renderForm("fixturesupply",data);						
					}else if(data.supplyType == "辅件厂商" ){
						renderForm("auxiliarysupply",data);							
					}else if(data.supplyType == "特色经贸商"){
						renderForm("strengsupply",data);																												
					}else if(data.supplyType== "整合厂商"){
						renderForm("mixsupply",data);												
					}else{
						
					}
				}
			});
		}
	}


</script>
