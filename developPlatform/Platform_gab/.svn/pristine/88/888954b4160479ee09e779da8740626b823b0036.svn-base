    function needImg() {
        var fileId = getFileId();
        $("#needImg").val(fileId);
        $("#imgDiv").filesUpload({
            pickerid: fileId,
            filecount: 1,
        });
    }

    function needFile() {
        var fileId = getFileId();
        $("#needFile").val(fileId);
        $("#fileDiv").filesUpload({
            pickerid: fileId,
            filecount: 1,
        });
    }

    function makingPlanFile() {
        var fileId = getFileId();
        $("#needPlan").val(fileId);
        $("#makingPlanDiv").filesUpload({
            pickerid: fileId,
            filecount: 1,
        });
    }

    // 服务需求的上传文件控件
    productDrawingFile();
    productImg();
    productFile();

    /* $("#productDrawingFile").hide();
