/*common*/
* {
    margin: 0;
    padding: 0;
    font-family: "Arial", "Microsoft YaHei", "黑体", "宋体", sans-serif;
}

body {
    font: 12px/1.5 "微软雅黑";
}

html, body {
    height: 100%;
    width: 100%;
    margin: 0px;
    padding: 0px;
}

a, img {
    border: none;
}

a {
    text-decoration: none;
    color: #333;
}

ul li, ol li {
    list-style: none;
}


.text-link {
    color: blue;
}

/*END index.html*/
/*公共样式*/
.hmq-padding-t-0 {
    padding-top: 0px;
}
.hmq-margin-lr-xs{
    margin-left:5px;
    margin-right:5px;
}

fieldset {
    border: solid 1px #aaa;
    margin: 5px;
}

fieldset .fieldset-body {
    padding: 5px;
}

.hideFieldset {
    border-left: 0;
    border-right: 0;
    border-bottom: 0;
}

.hideFieldset .fieldset-body {
    display: none;
}

/*END 公共样式*/


/*miniui样式覆盖与扩展*/
.mini-datagrid {
    width: 100%;
    height: 100%;
}

.mini-labelfield-label {
    line-height: 24px;
}
.mini-grid-emptyText{
    text-align: center !important;
}

/*DataGrid页面宽度增加*/
.mini-pager-size .mini-combobox {
    width: 65px !important;
}

.hmq-minix-icon {
    position: absolute;
    left: 5px;
    top: 50%;
    min-width: 16px;
    min-height: 16px;
    overflow: hidden;
    margin-top: -8px;
    +margin-top: -8px;
    display: inline-block;
    font: normal normal normal 14px/1 FontAwesome;
    text-rendering: auto;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    transform: translate(0, 0);
    font-size: 16px;
    line-height: 16px;
    text-align: center;
}

/*END DataGrid页面宽度增加*/


/*表单只读样式*/
.asLabel .mini-textbox-border,
.asLabel .mini-textbox-input,
.asLabel .mini-buttonedit-border,
.asLabel .mini-buttonedit-input,
.asLabel .mini-textboxlist-border {
    border: 0 !important;
    background: none !important;
    cursor: default !important;
}

.asLabel .mini-buttonedit-button,
.asLabel .mini-textboxlist-close {
    display: none !important;
}

.asLabel .mini-textboxlist-item {
    padding-right: 8px !important;
}

.asLabel .mini-textbox-input::placeholder, .asLabel .mini-buttonedit-input::placeholder {
    color: transparent;
    font-size: 0px;
}

.asLabel .mini-textbox-input::-webkit-input-placeholder, .asLabel.mini-buttonedit-input::-webkit-input-placeholder {
    color: transparent;
    font-size: 0px;
}

.asLabel .mini-textbox-input::-ms-input-placeholder, .asLabel .mini-buttonedit-input::-ms-input-placeholder {
    color: transparent;
    font-size: 0px;
}

.asLabel .mini-textbox-input::-moz-placeholder, .asLabel .mini-buttonedit-input::-moz-placeholder {
    color: transparent;
    font-size: 0px;
}


/*END 表单只读样式*/

/*END miniui样式覆盖与扩展*/

/*iframe报表*/
.hmq-iframe-box {
    width: 100%;
    overflow: hidden !important;
    border: 1px solid #999999;
}

.hmq-iframe-box > iframe {
    width: 100%;
    height: 100% !important;
    border: 0px;
    overflow: hidden;
}

/*END iframe报表*/

/*新增窗体表格自适应*/
table.hmq-modify-table,
table.hmq-modify-table > tr > th,
table.hmq-modify-table > tr > td,
table.hmq-modify-table > tbody > tr > th,
table.hmq-modify-table > tbody > tr > td {
    padding: 1px;
    padding: 0.2rem;
}

label.hmq-label{
    margin: 5px 10px 5px 0 !important;
    display: inline-block !important;
}
table.hmq-modify-table {
    margin: auto;
    border: 0;
    width: 100%;
    table-layout: fixed;
    _border-collapse: collapse;
    border-spacing: 1px;
    border-spacing: 0.2rem;

    width: 92%;
    padding-top: 0px;
    padding-bottom: 0px;
}

table.hmq-modify-table > tr > td,
table.hmq-modify-table > tbody > tr > td {
    height: 30px !important;
}

table.hmq-modify-table > tr > td[title],
table.hmq-modify-table > tr > td.title,
table.hmq-modify-table > tbody > tr > td[title],
table.hmq-modify-table > tbody > tr > td.title {
    white-space: nowrap;
    text-align: right;
    text-align-last: justify; /*字宽平均*/
}

table.hmq-modify-table > tr > td[content],
table.hmq-modify-table > tr > td.content,
table.hmq-modify-table > tbody tr > td[content],
table.hmq-modify-table > tbody tr > td.content {
    box-sizing: content-box !important;
}
tabel tr td label{
	margin: 0 5px 5px 0;
}

div.hmq-toolbar {
    padding: 5px 3px;
    padding: 0.5rem 3px;
    text-align: center;
    border-width: 0px;
}

div.hmq-toolbar > a.mini-button {
    margin-left: 5px;
    margin-right: 5px;
    min-width: 25px;
}

/*END 新增窗体表格自适应*/

/*查询页面参数自动换行*/
.hmq-search-td {
}

.hmq-search-td > span {
    display: inline-flex;
    display: inline-block;
    padding: 1px 0px;
}

/* Css Hack Chrome */
@media screen and (-webkit-min-device-pixel-ratio: 0) {
    .hmq-search-td > span {
        display: inline-flex;
        _padding: 0px;
    }
}

/*END 查询页面参数自动换行*/

/*textarea 列表页展示样式*/
.hmq-context-box {
    border: 1px solid #dddddd;
    box-sizing: border-box;
    width: inherit;
    max-width: 100%;
    height: 100%;
    max-height: 100%;

}

.hmq-context-box > * {
    padding-left: 20px;
    padding-right: 20px;
    font-weight: 500;
}

/*标题*/
.hmq-context-box > h1 {
    text-align: center;
    padding-top: 15px;
    padding-bottom: 5px;
}

/*描述*/
.hmq-context-box > h4 {
    text-align: right;
}

/*正文*/
.hmq-context-box > .hmq-context {
    text-indent: 2em;
    padding: 10px;
    word-break: break-word;
    font-size: 16px;
}

.hmq-context-box > .hmq-context > pre {
    overflow: auto;
    white-space: pre-wrap;
    /*以下代码为了兼容IE6,7*/
    *white-space: pre;
    *word-wrap: break-word;
}

/*textarea 列表页展示样式*/

