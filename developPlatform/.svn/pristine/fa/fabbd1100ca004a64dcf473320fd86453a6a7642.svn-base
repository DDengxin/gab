/*图片自适应*/
img{
    vertical-align: middle;
}
.hmq-img-responsive {
    display: block;
    height: auto;
    max-width: 100%;
}
.hmq-img-rounded{
    border-radius: 6px;
}
.hmq-img-circle{
    border-radius: 50%;
}
.hmq-img-thumbnail{
    display: inline-block;
    max-width: 100%;
    height: auto;
    padding: 4px;
    line-height: 1.42857143;
    background-color: #fff;
    border: 1px solid #ddd;
    border-radius: 4px;
    -webkit-transition: all .2s ease-in-out;
    -o-transition: all .2s ease-in-out;
    transition: all .2s ease-in-out;
}
/*END 图片自适应*/
.hmq-text-center{
    text-align: center;
}

