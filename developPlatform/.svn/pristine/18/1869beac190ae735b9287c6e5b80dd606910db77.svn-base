
.tab-bar{
	overflow: hidden;
    position: fixed;
    top: 50px;
    z-index: 99;
    width: 150px;
	}
.tab_li{
	width:33.3333%;
	height:30px;
	line-height:30px;
	color:#fff;
	font-size:15px;
	font-weight:bold;
	text-align:center;
	float: left;
}
.tar-btn{
	display:block;
	width:100%;
	height:30px;
	line-height:30px;
	color:#fff;
	font-size:15px;
	font-weight:bold;
	text-align:center;
    cursor: pointer;
}
.tab-left{
	background:#e59909;
	float:left
}
.tab-right{
	background:#4093fe;
	float:right
}
.tab-page-box{
	padding-top: 20px;
}
.current{
	display:none
}
.fa_center{
	position: relative;
    left: 50%;
    transform: translateX(-50%);
}
