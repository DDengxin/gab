        }else if (Integer.parseInt(ck_count)>0){//是否正在出库
        	rmap.put("status", false);
        	rmap.put("message", "该包装正在出库！");
        }else {
        	rmap.put("data",eCkOutDao.QueryListMap("select * from v_ck_mx where pack = '"+pack+"' ", null));
        	rmap.put("status", true);
