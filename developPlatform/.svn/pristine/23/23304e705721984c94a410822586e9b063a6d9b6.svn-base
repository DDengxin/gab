package com.tengzhi.base.security.common.property.detail;

public class BrowserProperty {
    /**
     * 同一个用户在系统中最大的session数，默认1
     */
    private int maxSessions = 1;
    /**
     * session失效/被踢掉时跳转的地址，默认为系统默认登陆页
     */
    private String sessionFailureUrl;
    /**
     * 退出登录接口
     */
    private String logoutUrl = "/logout";
    /**
     * 退出URL
     */
    private String logoutSuccessUrl;
    /**
     * 记住我(时间)
     */
    private int seconds = 3600;
    /**
     * 登陆
     */
    private String loginPage;
    /**
     * 系统首页URL
     */
    private String homePage;
    /**
     * ajax
     */
    private String loginProcessingUrl;
    /**
     * 关闭csrf验证
     */
    private boolean iframeStatus = true;
    /**
     * 允许iframe在同域内跳转
     */
    private boolean sameOrigin = true;
    /**
     * 记住我
     */
    private boolean rememberMe = false;
    /**
     * 禁用缓存
     */
    private boolean cache = true;

    /**
     * 403错误跳转页面URL
     */
    private String forbidden_page;
    /**
     * 404错误跳转页面URL
     */
    private String not_found_page;
    /**
     * 500错误跳转页面URL
     */
    private String internal_server_error_page;

    public int getMaxSessions() {
        return maxSessions;
    }

    public void setMaxSessions(int maxSessions) {
        this.maxSessions = maxSessions;
    }

    public String getSessionFailureUrl() {
        return sessionFailureUrl;
    }

    public void setSessionFailureUrl(String sessionFailureUrl) {
        this.sessionFailureUrl = sessionFailureUrl;
    }

    public String getLogoutUrl() {
        return logoutUrl;
    }

    public void setLogoutUrl(String logoutUrl) {
        this.logoutUrl = logoutUrl;
    }

    public String getLogoutSuccessUrl() {
        return logoutSuccessUrl;
    }

    public void setLogoutSuccessUrl(String logoutSuccessUrl) {
        this.logoutSuccessUrl = logoutSuccessUrl;
    }

    public int getSeconds() {
        return seconds;
    }

    public void setSeconds(int seconds) {
        this.seconds = seconds;
    }

    public boolean isIframeStatus() {
        return iframeStatus;
    }

    public void setIframeStatus(boolean iframeStatus) {
        this.iframeStatus = iframeStatus;
    }

    public String getLoginPage() {
        return loginPage;
    }

    public void setLoginPage(String loginPage) {
        this.loginPage = loginPage;
    }

    public String getLoginProcessingUrl() {
        return loginProcessingUrl;
    }

    public void setLoginProcessingUrl(String loginProcessingUrl) {
        this.loginProcessingUrl = loginProcessingUrl;
    }

    public boolean isSameOrigin() {
        return sameOrigin;
    }

    public void setSameOrigin(boolean sameOrigin) {
        this.sameOrigin = sameOrigin;
    }

    public boolean isCache() {
        return cache;
    }

    public void setCache(boolean cache) {
        this.cache = cache;
    }

    public boolean isRememberMe() {
        return rememberMe;
    }

    public void setRememberMe(boolean rememberMe) {
        this.rememberMe = rememberMe;
    }

    public String getHomePage() {
        return homePage;
    }

    public void setHomePage(String homePage) {
        this.homePage = homePage;
    }

	public String getForbidden_page() {
		return forbidden_page;
	}

	public void setForbidden_page(String forbidden_page) {
		this.forbidden_page = forbidden_page;
	}

	public String getNot_found_page() {
		return not_found_page;
	}

	public void setNot_found_page(String not_found_page) {
		this.not_found_page = not_found_page;
	}

	public String getInternal_server_error_page() {
		return internal_server_error_page;
	}

	public void setInternal_server_error_page(String internal_server_error_page) {
		this.internal_server_error_page = internal_server_error_page;
	}
}
