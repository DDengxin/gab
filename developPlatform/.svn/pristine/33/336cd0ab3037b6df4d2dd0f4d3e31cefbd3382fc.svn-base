package com.tengzhi.business.finance.reportItem.model;

import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;
import javax.persistence.Transient;
import java.math.BigInteger;
import java.util.UUID;

@Entity
@Table(name = "e_f_voucher_reportitem")
public class EFVoucherReportitem {

  private String freportitem;
  private String fdesc;
  private String fnature;
  private Integer fdc;
  private BigInteger fgroupid;
  private Integer forder;
  private Integer frownum;
  private Integer fdatasource;
  private String fformula;
  private String dataCorp;
  private BigInteger freportid;
  private Integer fcolumn;
  private Integer fnumcolumn;
  private String sm;
  @Id
  private String sid = UUID.randomUUID().toString();


  public String getFreportitem() {
    return freportitem;
  }

  public void setFreportitem(String freportitem) {
    this.freportitem = freportitem;
  }


  public String getFdesc() {
    return fdesc;
  }

  public void setFdesc(String fdesc) {
    this.fdesc = fdesc;
  }


  public String getFnature() {
    return fnature;
  }

  public void setFnature(String fnature) {
    this.fnature = fnature;
  }


  public Integer getFdc() {
    return fdc;
  }

  public void setFdc(Integer fdc) {
    this.fdc = fdc;
  }


  public BigInteger getFgroupid() {
    return fgroupid;
  }

  public void setFgroupid(BigInteger fgroupid) {
    this.fgroupid = fgroupid;
  }


  public Integer getForder() {
    return forder;
  }

  public void setForder(Integer forder) {
    this.forder = forder;
  }


  public Integer getFrownum() {
    return frownum;
  }

  public void setFrownum(Integer frownum) {
    this.frownum = frownum;
  }


  public Integer getFdatasource() {
    return fdatasource;
  }

  public void setFdatasource(Integer fdatasource) {
    this.fdatasource = fdatasource;
  }


  public String getFformula() {
    return fformula;
  }

  public void setFformula(String fformula) {
    this.fformula = fformula;
  }


  public String getDataCorp() {
    return dataCorp;
  }

  public void setDataCorp(String dataCorp) {
    this.dataCorp = dataCorp;
  }


  public BigInteger getFreportid() {
    return freportid;
  }

  public void setFreportid(BigInteger freportid) {
    this.freportid = freportid;
  }


  public Integer getFcolumn() {
    return fcolumn;
  }

  public void setFcolumn(Integer fcolumn) {
    this.fcolumn = fcolumn;
  }


  public Integer getFnumcolumn() {
    return fnumcolumn;
  }

  public void setFnumcolumn(Integer fnumcolumn) {
    this.fnumcolumn = fnumcolumn;
  }


  public String getSm() {
    return sm;
  }

  public void setSm(String sm) {
    this.sm = sm;
  }


  public String getSid() {
    return sid;
  }

  public void setSid(String sid) {
    this.sid = sid;
  }

  @Transient
  private String fdcName;

  public String getFdcName() {
    return fdcName;
  }

  public void setFdcName(String fdcName) {
    this.fdcName = fdcName;
  }
}
